<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.xored.javafx.packeteditor.controllers.FieldEditorController"
            fx:id="fieldEditorBorderPane"
            styleClass="fieldenginetoppane">
    <top>
        <FlowPane xmlns:fx="http://javafx.com/fxml/1"
                  fx:id="fieldEditorTopPane"
                  styleClass="fieldeditortoppane">
        </FlowPane>
    </top>
    <center>
        <ScrollPane xmlns:fx="http://javafx.com/fxml/1"
                   fx:id="fieldEditorScrollPane"
                   styleClass="fieldeditorscrollpane">
            <StackPane xmlns:fx="http://javafx.com/fxml/1"
                       fx:id="fieldEditorCenterPane"
                       prefWidth="800"
                       styleClass="stackpane"
            />
        </ScrollPane>
    </center>
    <bottom>
        <StackPane xmlns:fx="http://javafx.com/fxml/1"
                   fx:id="fieldEditorBottomPane"
                   prefWidth="800"
                   styleClass="stackpane"
        />
    </bottom>
</BorderPane>